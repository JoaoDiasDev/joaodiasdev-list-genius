<MudNavMenu>
    <MudNavLink Href="/" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Home">
        Home
    </MudNavLink>
    <AuthorizeView>
        <Authorized>
            <MudNavLink Href="User/Manage" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.List">
                @context.User.Identity?.Name
            </MudNavLink>
            <MudNavLink Href="ProductsLists" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.ViewList">
                Shopping Lists
            </MudNavLink>
        </Authorized>
        <NotAuthorized>
            <MudNavLink Href="Login" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.List">
                Login
            </MudNavLink>
        </NotAuthorized>
    </AuthorizeView>
</MudNavMenu>

